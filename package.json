{
  "name": "serverless-lambda-api-boilerplate",
  "version": "1.0.0",
  "description": "a serverless lambda api boilerplate",
  "scripts": {
    "prestart": "npm run build",
    "start": "sls offline start --skipCacheInvalidation",
    "prebuild": "npm run test",
    "build": "babel src -d dist",
