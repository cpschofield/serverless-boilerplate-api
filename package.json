    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "babel-changed": "^8.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
